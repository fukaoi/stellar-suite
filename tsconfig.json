{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "lib": [ "ES2019", "DOM"],
    "sourceMap": false,
    "outDir": "./dist",
    "moduleResolution": "node",  
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./src/**/__test__"
  ]
}
