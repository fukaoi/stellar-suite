		"stellar-sdk": "^6.0.0",
		"ts-loader": "^8.0.9",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.5",
		"web3-bzz": "^1.3.0",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11",
